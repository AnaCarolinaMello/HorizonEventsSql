 <div id="flashMsg">
    {{#if success_msg}}
    <div class="alert alert-success">
      <p>{{success_msg}}</p>
      <p class="btnClose">X</p>
    </div>
    {{/if}}

    {{#if error_msg}}
    <div class="alert alert-danger">
      <p>{{error_msg}}</p>
      <p class="btnClose">X</p>
    </div>
    {{/if}}
    {{#if warning_msg}}
    <div class="alert alert-warning">
      <p>{{warning_msg}}</p>
      <p class="btnClose">X</p>
    </div>
    {{/if}}
  </div>
